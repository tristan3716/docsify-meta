{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4]
    },
    "globals": {
        "Docsify": "readonly",
        "$docsify": "readonly"
    }
}
